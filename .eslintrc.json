{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": "warn",
      "react/prop-types": "off",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "eqeqeq": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  